{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "title": "Percent of Year 7-9 students who reported being bullied recently (2016)",
    "width": "container",
    "height": 450,
    "projection": {
      "type": "conicEqualArea",
      "parallels": [
        -36,
        -38
      ],
      "rotate": [
        -145,
        0,
        0
      ]
    },
    "data": {
      "url": "https://raw.githubusercontent.com/cywng/FIT3179/main/Data/VICLGAtopojson.json",
      "format": {
        "type": "topojson",
        "feature": "VICLGAgeojson"
      }
    },
    "encoding": {
      "tooltip": [
        {
          "field": "properties.vic_lga__3",
          "type": "nominal",
          "title": "Local Government Area"
        },
        {
          "field": "Portion Bullied",
          "type": "quantitative",
          "title": "Percent Bullied"
        }
      ]
    },
    "layer": [
      {
        "mark": {
          "type": "geoshape",
          "fill": "gainsboro",
          "stroke": "white"
        }
      },
      {
        "transform": [
          {
            "lookup": "properties.vic_lga__3",
            "from": {
              "data": {
                "url": "https://raw.githubusercontent.com/elim0055/3179/main/Data/FilteredData.csv?token=GHSAT0AAAAAACIXLQABODTNS7L5CZWLUDJOZJM5IKA"
              },
              "key": "LGA",
              "fields": [
                "Portion Bullied"
              ]
            }
          }
        ],
        "mark": {
          "type": "geoshape"
        },
        "encoding": {
          "color": {
            "field": "Portion Bullied",
            "type": "quantitative",
            "title": [
              "Proportion of",
              "students bullied"
            ],
            "scale": {
              "type": "threshold",
              "domain": [10,18,26],
              "range": ["#c6dbef","#6baed6","#2171b5","#084594"]
            }
          }
        }
      }
    ],
    "config": {
      "view": {
        "stroke": null
      },
      "axis": {
        "labelFont": "Oxygen",
        "titleFont": "Oxygen"
      },
      "legend": {
        "labelFont": "Oxygen",
        "titleFont": "Oxygen"
      },
      "header": {
        "labelFont": "Oxygen",
        "titleFont": "Oxygen"
      },
      "title": {
        "font": "Oxygen",
        "fontSize": 16,
        "fontWeight": 600
      }
    }
  }